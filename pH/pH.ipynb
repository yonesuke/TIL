      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "pylab.figure(figsize=[8,6])\n",
    "pylab.xlim(0,40)\n",
    "pylab.ylim(0,14)\n",
    "pylab.plot(x,ph)\n",
    "pylab.savefig(\"ph.jpg\")"
   ]
