    "import pylab\n",
    "import numpy as np\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "ph = np.linspace(1,13,num=10**5)\n",
    "h = np.power(0.1,ph)\n",
    "oh = 10**(-14)/h\n",
    "x = -20*(h-oh-0.1)/(h-oh+0.1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "metadata": {},
   "outputs": [
